#include<stdio.h>
int main()
{
    int i=0;
    int n=0;
    int q=0;
    int m=0;
    scanf("%d",&q);
    printf("\033c");
    for(i=2;i<=10000000000;i++)
    {
        int a=0;
        for(n=2;n<i;n++)
        {
            if(i%n==0)
            {
                a++;
                break;
            }
        }
        if(a==0)
        {
           m++;
        while(q==m)
        {
            printf("%d番目の素数は%dです。\n",q,i);
            goto here;
        }
        }
    }
    here:
    return 0;
}
